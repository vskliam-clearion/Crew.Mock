<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Crew.Mock.Views.ApplicationCodeSetupPage"
             AutomationId="applicationSetupPage">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" VerticalOptions="Center" AutomationId="applicationCodeSetupPage">
            <Button Text="Enter App Code"
                    Clicked="EnterAppCodeButtonClicked"
                    AutomationId="enterAppCodeButton"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>