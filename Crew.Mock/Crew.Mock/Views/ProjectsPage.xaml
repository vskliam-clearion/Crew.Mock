<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Crew.Mock.Views.ProjectsPage">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" VerticalOptions="Center">
            <Button Text="GoBack"
                Clicked="Button_Clicked"/>
            <Button Text="Generate Crash"
                Clicked="Button_Clicked_1"/>
            <Button Text="Generate unhandled exception"
                Clicked="Button_Clicked_2"/>
            <Button Text="Generate handled exception"
                Clicked="Button_Clicked_3"/>
            <Button Text="Generate handled exception with parameters"
                Clicked="Button_Clicked_4"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>